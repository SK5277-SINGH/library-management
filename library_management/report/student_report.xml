<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <template id="report_studentdetail">
    <t t-call="web.external_layout">
        <!-- <t t-set="doc" t-value="doc.with_context({})" />  -->
        <div class="page">
            <div class="oe_structure"/>
            <div class="row">
                <table width="100%">
                    <tr>
                        <td class="text-center"><b> Student Details</b></td>
                    </tr>
                </table>
                <table class="table table-bordered mb32" style="table-layout:auto" width='100%'>
                    <thead>
                        <tr>
                            <th>Name</th>
                            <th>Class</th>
                            <th>Division</th>
                            <th>Gender</th>
                            <th>Date</th>
                        </tr>

                        <t t-foreach="docs" t-as="doc">
                            <tr>
                               <td><t t-raw="doc.name"/></td>
                               <td><t t-raw="doc.course_ids.course"/></td>
                               <td><t t-raw="doc.division_ids.name"/></td>
                               <td><t t-raw="doc.gender"/></td>
                               <td><t t-raw="doc.date"/></td>
                            </tr>
                        </t>
                    </thead>
               </table>
            </div>
        </div>
    </t>
    </template>

